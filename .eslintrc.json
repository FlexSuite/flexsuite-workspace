{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals"
  ],
  "rules": { 
    "no-console": "warn",
    "semi":["error","never"],
    "prefer-arrow-callback": "error",
    "@typescript-eslint/no-explicit-any": "off"
  }
}